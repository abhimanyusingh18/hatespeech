{
    "name": "Hate Speech Detector And Blocker",
    "version": "1.0.1",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["content.js"]
        }
    ],
    "permissions": [
        "activeTab",
        "tts",
        "storage"
        ],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
        "128":"logo.png"
        }
    }
}